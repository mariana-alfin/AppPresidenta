<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.apppresidenta">

    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/icono_app"
        android:label="@string/app_name"
        android:networkSecurityConfig="@xml/network_security_config"
        android:roundIcon="@mipmap/icono_app_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppPresidenta">
        <activity
            android:name=".JuntaActivity"
            android:exported="false"
            android:screenOrientation="portrait" />
        <activity
            android:name=".DetalleClienteActivity"
            android:exported="false"
            android:screenOrientation="portrait" />
        <activity
            android:name=".HistorialActivity"
            android:exported="false"
            android:screenOrientation="portrait" />
        <activity
            android:name=".CalculadoraActivity"
            android:exported="false"
            android:screenOrientation="portrait" />
        <activity
            android:name=".Navegacion"
            android:exported="false"
            android:label="@string/title_activity_navegacion"
            android:screenOrientation="portrait" />
        <activity
            android:name=".MainActivity"
            android:exported="false"
            android:screenOrientation="portrait" />
        <activity
            android:name=".InicioApp"
            android:exported="true"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>